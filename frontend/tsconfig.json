{
	"compilerOptions": {
		"module": "ESNext",
		"moduleResolution": "bundler",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "react-jsx",
		"types": ["vite/client"],
		"baseUrl": ".",
		"paths": {
			"@src/*": ["./src/*"],
			"@components/*": ["./src/components/*"],
			"@styles/*": ["./src/styles/*"]
		}
	}
}
